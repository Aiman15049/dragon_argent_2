
<h1 class="cms-header"><i class="material-icons"></i>Dynamic Pages</h1>
<% if @dynamic_pages.any? %>
  <table class="table table-striped table-responsive">
    <thead class="table-inverse">
      <th> Intro</th>
      <th> Title</th>
      <th> Meta Title</th>
      <th> Meta Description</th>
      <th colspan=3> Actions</th>
    </thead>
    <tbody id='index_rows'>
      <%= render "cms/dynamic_pages/partials/index_rows" %>
    </tbody>
</table>
<% else %>
  <div>There are no dynamic_pages on the system. Why not add one <%= link_to 'here?', new_cms_dynamic_page_path %></div>
<% end %>

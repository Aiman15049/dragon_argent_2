<% dynamic_panel_types.each do |dynamic_panel_type| %>
  <tr>
    <td nowrap='nowrap'>
      <%= link_to move_to_bottom_cms_dynamic_panel_type_path(dynamic_panel_type), data: { remote: true } do %>
        <i class="material-icons">file_download</i>
      <% end %>
      <%= link_to move_lower_cms_dynamic_panel_type_path(dynamic_panel_type), data: { remote: true } do %>
        <i class="material-icons">arrow_drop_down</i>
      <% end %>
      <%= link_to move_higher_cms_dynamic_panel_type_path(dynamic_panel_type), data: { remote: true } do %>
        <i class="material-icons">arrow_drop_up</i>
      <% end %>
      <%= link_to move_to_top_cms_dynamic_panel_type_path(dynamic_panel_type), data: { remote: true } do %>
        <i class="material-icons">file_upload</i>
      <% end %>
    </td>
    <td><%= dynamic_panel_type.title %></td>
    <td><%= dynamic_panel_type.cms_name %></td>
    <td>
    </td>
    <td class="table-link">
      <%= link_to edit_cms_dynamic_panel_type_path(dynamic_panel_type) do %>
        <i class="material-icons">edit</i>
      <% end %>
    </td>
    <td class="table-link">
      <%= link_to cms_dynamic_panel_type_path(dynamic_panel_type), method: :delete, data: { confirm: 'Are you sure you want to delete this Dynamic Panel Type?' } do %>
        <i class="material-icons">delete</i>
      <% end %>
    </td>
  </tr>
<% end %>
